<%= form_with(model: score, local: true) do |form| %>
  <% if score.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(score.errors.count, "error") %> prohibited this score from being saved:</h2>

      <ul>
      <% score.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :tittle %><br>
    <%= form.text_field :tittle %>
  </div>

  <div class="field">
    <%= form.label :key %><br>
    <%= form.select :key,[["C", "C"], ["C#/Db", "C#/Db"],["D", "D"], ["D#/Eb", "D#/Eb"],["E", "E"], ["F", "F"],["F#/Gb", "F#/Gb"], ["G", "G"], ["G#/Ab", "G#/Ab"],["A", "A"], ["A#/Bb", "A#/Bb"],["B", "B"]] %>
  </div>

  <div class="field">
    <%= form.label :chord %><br>
      <%= form.text_area :chord %>
  </div>


  <div class="actions">
    <%= form.submit class: 'btn btn-success' %>
  </div>
<% end %>
